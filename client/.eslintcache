}\n      {isLoading ? (\n        <div>Retrieving Sales Amount Variation Chart ...</div>\n      ) : (\n        <>\n          <span>Increase & decrease sales amount per day</span>\n          <br></br>\n          {percentloading ? (\n            <div>Retrieving Sales Percentage</div>\n          ) : (\n            <p>{parseFloat(salesPercent.data.percentage).toFixed(2)} </p>\n          )}\n          <br></br>\n          <h3>\n            {salesPerDay.data.reduce(\n              (a, b) => a.totalSaleAmount + b.totalSaleAmount\n            )}\n          </h3>\n\n          <Chart data={salesPerDay.data} className={classes.chart}>\n            <ArgumentScale factory={scalePoint} />\n            <ArgumentAxis />\n            <ValueAxis />\n\n            <AreaSeries\n              name=\"Sales Amount\"\n              valueField=\"totalSaleAmount\"\n              argumentField=\"_id\"\n              seriesComponent={Area}\n            />\n\n            <Animation />\n            <Legend\n              position=\"bottom\"\n              rootComponent={Root}\n              labelComponent={Label}\n            />\n            <Title text=\"Sales Amount Variation Chart\" />\n          </Chart>\n        </>\n      )}\n    </Paper>\n  );\n};\n","/home/adminit/work/ecom-chart/data-vizualization/client/src/component/orders-variation.js",["62","63"],"/home/adminit/work/ecom-chart/data-vizualization/client/src/component/products-chart.js",["64","65","66","67","68","69","70","71"],"/home/adminit/work/data-vizualization/client/src/index.js",[],["72","73"],"/home/adminit/work/data-vizualization/client/src/App.js",["74","75"],"import ProductChart from \"./component/products-chart\";\nimport SalesVariation from \"./component/sales-variation\";\nimport OrdersVariation from \"./component/orders-variation\";\nimport { BrowserRouter as Router, Switch, Route, Link } from \"react-router-dom\";\n\nimport { QueryClient, QueryClientProvider, useQuery } from \"react-query\";\nimport { ReactQueryDevtool